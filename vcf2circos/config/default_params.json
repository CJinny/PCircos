            "sortbycolor": "True",
            "colorcolumn": 3,
            "hovertextformat": " \"Chromosome: {}<br>Start: {}<br>End: {}<br>Value: {:.4f}\".format(a[i,0], a[i,1], a[i,2], float(a[i,3]))",
            "palatte": {
                "palatte": "RdBu",
                "scale": "div",
                "reverse": "True",
                "ncolor": 11
            },
            "trace": {
                "hoverinfo": "text",
                "mode": "markers",
                "marker": {
                    "size": 0,
                    "opacity": 0
                }
            },
            "layout": {
                "type": "path",
                "layer": "above",
                "opacity": 0.8,
                
                "line": {
                    "color": "black",
                    "width": 0
                }
            }
        },
        "link": {
            "show": "True",
            "file": {
                "path": "/path/to/your/link/data",
                "header": "infer",
                "sep": "\t"
            },
            "radius": {
                "R0": 0,
                "R1": 0.45
            },
            "sortbycolor": "False",
            "colorcolumn": "None",
            "hovertextformat": [
                " \"Chromosome: {} => {}<br>From: {} => {}<br>From: {} => {}\".format(a[i,0], a[i,3], a[i,1], a[i,4], a[i,2], a[i,5]) ",
                " \"Chromosome: {} => {}<br>From: {} => {}<br>From: {} => {}\".format(a[i,3], a[i,0], a[i,4], a[i,1], a[i,5], a[i,2]) "
            ],
            "trace": {
                "hoverinfo": "text",
                "mode": "markers",
                "marker": {
                    "size": 0,
                    "opacity": 0
                }
            },
            "layout": {
                "type": "path",
                "layer": "below",
                "opacity": 1,
                "line": {
                    "color": "blue",
                    "width": 0.5
                }
            }
        },
        "ribbon": {
