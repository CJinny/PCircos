{
    "General": {
        "width": 1500,
        "height": 1500,
        "title": "my Circos plot",
        "titlefont": {
            "family": "Times New Roman",
            "size": 18,
            "color": "black"
        },
        "hovermode": "closest",
        "showlegend": "False",
        "xaxis": {
            "range": [
                -1.4,
                1.4
            ],
            "autorange": "False",
            "showgrid": "False",
            "zeroline": "False",
            "showticklabels": "False"
        },
        "yaxis": {
            "range": [
                -1.4,
                1.4
            ],
            "autorange": "False",
            "showgrid": "False",
            "zeroline": "False",
            "showticklabels": "False"
        }
    },
    "Category": {
        "ideogram": {
            "patch": {
                "file": {
                    "path": "/path/to/chr_size_info/data",
                    "header": "infer",
                    "sep": "\t"
                },
                "show": "True",
                "degreerange": [
                    0,
                    360
                ],
                "showfillcolor": "False",
                "chrannotation": {
                    "show": "True",
                    "radius": {
                        "R": 1.2
                    },
                    "fonttype": "bold",
                    "textangle": {
                        "angleoffset": -90,
                        "anglelimit": 360
                    },
                    "layout": {
                        "xref": "x",
                        "yref": "y",
                        "showarrow": "False",
                        "font": {
                            "size": 15,
                            "color": "black"
                        }
                    }
                },
                "customoptions": {
                    "customlabel": "False",
                    "customspacing": "False",
                    "customcolor": "False"
                },
                "npoints": 1000,
                "radius": {
                    "R0": 1.0,
                    "R1": 1.1
                },
                "layout": {
                    "type": "path",
                    "opacity": 0.9,
                    "layer": "above",
                    "line": {
                        "color": "black",
                        "width": 2
                    }
                }
            },
            "majortick": {
                "show": "False",
                "spacing": 20000000,
                "radius": {
                    "R0": 1.1,
                    "R1": 1.12
                },
                "layout": {
                    "type": "path",
                    "opacity": 0.9,
                    "layer": "above",
                    "line": {
                        "color": "black",
                        "width": 1
                    }
                }
            },
            "minortick": {
                "show": "False",
                "spacing": 5000000,
                "radius": {
                    "R0": 1.1,
                    "R1": 1.108
                },
                "layout": {
                    "type": "path",
                    "opacity": 0.9,
                    "line": {
                        "color": "black",
                        "width": 0.5
                    }
                }
            },
            "ticklabel": {
                "show": "False",
                "spacing": 20000000,
                "radius": {
                    "R": 1.16
                },
                "textformat": "Mb",
                "textangle": {
                    "angleoffset": -90,
                    "anglelimit": 360
                },
                "layout": {
                    "xref": "x",
                    "yref": "y",
                    "showarrow": "False",
                    "font": {
                        "family": "Times New Roman",
                        "size": 10,
                        "color": "black"
                    }
                }
            }
        },
        "cytoband": {
            "show": "True",
            "file": {
                "path": "/path/to/your/cytoband/interval/data",
                "header": "infer",
                "sep": "\t"
            },
            "sortbycolor": "True",
            "colorcolumn": 3,
            "layout": {
                "type": "path",
                "layer": "below",
                "opacity": 1.0,
                "line": {
                    "color": "black",
                    "width": 0
                }
            }
        },
        "histogram": {
            "show": "True",
            "customfillcolor": "False",
            "file": {
                "path": "/path/to/your/histogram/interval/data",
                "header": "infer",
                "sep": "\t"
            },
            "sortbycolor": "False",
            "colorcolumn": "None",
            "radius": {
                "R0": 0.90,
                "d": 0.06
            },
            "hovertextformat": " \"Chromosome: {}<br>Start: {}<br>End: {}<br>LogFC: {:.4f}\".format(a[i,0], a[i,1], a[i,2], float(a[i,3])) ",
            "trace": {
                "hoverinfo": "text",
                "mode": "markers",
                "marker": {
                    "size": 0,
                    "opacity": 0
                }
            },
            "layout": {
                "type": "path",
                "opacity": 1,
                "fillcolor": "red",
                "line": {
                    "color": "black",
                    "width": 0
                }
            }
        },
        "line": {
            "show": "True",
            "file": {
                "path": "/path/to/your/line/point/data",
                "header": "infer",
                "sep": "\t"
            },
            "radius": {
                "R0": 0,
                "R1": 0
            },
            "sortbycolor": "False",
            "colorcolumn": "None",
            "hovertextformat": " \"Chromosome: {}<br>Position: {}<br>Value: {:.2f}\".format(a[i,0], a[i,1], float(a[i,2]))",
            "trace": {
                "hoverinfo": "text",
                "mode": "lines+markers",
                "opacity": 0.9,
                "marker": {
                    "symbol": 0,
                    "size": 3,
                    "color": "black"
                },
                "line": {
                    "color": "black",
                    "width": 2,
                    "shape": "linear",
                    "smoothing": 0
                }
            }
        },
        "area": {
            "show": "False",
            "file": {
                "path": "/path/to/your/line/point/data",
                "header": "infer",
                "sep": "\t"
            },
            "radius": {
                "R0": 0,
                "R1": 0
            },
            "sortbycolor": "False",
            "colorcolumn": "None",
            "hovertextformat": " \"Chromosome: {}<br>Position: {}<br>Value: {:.2f}\".format(a[i,0], a[i,1], float(a[i,2]))",
            "trace": {
                "hoverinfo": "text",
                "mode": "markers",
                "marker": {
                    "size": 0,
                    "opacity": 0
                }
            },
            "layout": {
                "type": "path",
                "layer": "above",
                "opacity": 0.2,
                "fillcolor": "grey",
                "line": {
                    "color": "black",
                    "width": 0
                }
            }
        },
        "scatter": {
            "show": "True",
            "file": {
                "path": "/path/to/your/scatter/point/data",
                "header": "infer",
                "sep": "\t"
            },
            "radius": {
                "R0": 0,
                "R1": 0
            },
            "sortbycolor": "False",
            "colorcolumn": "ideogram",
            "hovertextformat": " \"Chromosome: {}<br>Position: {}<br>Value: {:.2f}\".format(a[i,0], a[i,1], float(a[i,2]))",
            "trace": {
                "hoverinfo": "text",
                "mode": "markers",
                "marker": {
                    "size": 5,
                    "symbol": 0,
                    "opacity": 0.9,
                    "color": "red"
                }
            }
        },
        "tile": {
            "show": "True",
            "file": {
                "path": "/path/to/your/tile/interval/data",
                "header": "infer",
                "sep": "\t"
            },
            "sortbycolor": "False",
            "colorcolumn": "None",
            "radius": {
                "R0": 0,
                "R1": 0
            },
            "hovertextformat": " \"Chromosome: {}<br>Start: {}<br>End: {}<br>CNV: {:.4f}\".format(a[i,0], a[i,1], a[i,2], float(a[i,3]))",
            "trace": {
                "hoverinfo": "text",
                "mode": "markers",
                "marker": {
                    "size": 0,
                    "opacity": 0
                }
            },
            "layout": {
                "type": "path",
                "layer": "above",
                "opacity": 0.9,
                "line": {
                    "color": "black",
                    "width": 3,
                    "dash": "solid"
                }
            }
        },
        "heatmap": {
            "show": "True",
            "file": {
                "path": "/path/to/your/heatmap/interval/data",
                "header": "infer",
                "sep": "\t"
            },
            "radius": {
                "R0": 0,
                "R1": 0
